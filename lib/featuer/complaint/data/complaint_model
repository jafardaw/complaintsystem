  // ğŸ’¡ Ù†Ù…ÙˆØ°Ø¬ Ù„ØªÙ…Ø«ÙŠÙ„ Ø¨ÙŠØ§Ù†Ø§Øª Ø§Ù„Ø´ÙƒÙˆÙ‰ Ø§Ù„Ù…Ø±ÙÙ‚Ø© ÙÙŠ Ø§Ù„Ø±Ø¯
  class Complaint {
    final int id;
    final String referenceCode;
    final String title;
    final String description;
    final String locationText;
    final String status;
    final String priority;
    final String createdAt;
    final Agency agency;
    final List<Attachment> attachments;

    Complaint({
      required this.id,
      required this.referenceCode,
      required this.title,
      required this.description,
      required this.locationText,
      required this.status,
      required this.priority,
      required this.createdAt,
      required this.agency,
      required this.attachments,
    });

    factory Complaint.fromJson(Map<String, dynamic> json) {
      return Complaint(
        id: json['id'] as int,
        referenceCode: json['reference_code'] as String,
        title: json['title'] as String,
        description: json['description'] as String,
        locationText: json['location_text'] as String,
        status: json['status'] as String,
        priority: json['priority'] as String,
        createdAt: json['created_at'] as String,
        agency: Agency.fromJson(json['agency'] as Map<String, dynamic>),
        attachments: (json['attachments'] as List<dynamic>?)
                ?.map((e) => Attachment.fromJson(e as Map<String, dynamic>))
                .toList() ??
            [],
      );
    }
  }

  // ğŸ’¡ Ù†Ù…ÙˆØ°Ø¬ Ø§Ù„Ù‡ÙŠØ¦Ø© Ø§Ù„Ù…Ø±ÙÙ‚ Ø¯Ø§Ø®Ù„ Ø§Ù„Ø´ÙƒÙˆÙ‰
  class Agency {
    final int id;
    final String name;

    Agency({required this.id, required this.name});

    factory Agency.fromJson(Map<String, dynamic> json) {
      return Agency(
        id: json['id'] as int,
        name: json['name'] as String,
      );
    }
  }

  // ğŸ’¡ Ù†Ù…ÙˆØ°Ø¬ Ø§Ù„Ù…Ø±ÙÙ‚Ø§Øª
  class Attachment {
    final int id;m
    final String filePath;
    final String fileType;

    Attachment({required this.id, required this.filePath, required this.fileType});

    factory Attachment.fromJson(Map<String, dynamic> json) {
      return Attachment(
        id: json['id'] as int,
        filePath: json['file_path'] as String,
        fileType: json['file_type'] as String,
      );
    }
  }

  // ğŸ’¡ Ù†Ù…ÙˆØ°Ø¬ Ø§Ù„Ø§Ø³ØªØ¬Ø§Ø¨Ø© Ø§Ù„ÙƒØ§Ù…Ù„ Ù…Ø¹ Ø¨ÙŠØ§Ù†Ø§Øª Ø§Ù„ØªØ±Ù‚ÙŠÙ…
  class ComplaintsResponse {
    final int currentPage;
    final List<Complaint> complaints;
    final int lastPage;
    final String? nextPageUrl;
    final String? prevPageUrl;
    final int total;

    ComplaintsResponse({
      required this.currentPage,
      required this.complaints,
      required this.lastPage,
      this.nextPageUrl,
      this.prevPageUrl,
      required this.total,
    });

    factory ComplaintsResponse.fromJson(Map<String, dynamic> json) {
      return ComplaintsResponse(
        currentPage: json['current_page'] as int,
        lastPage: json['last_page'] as int,
        total: json['total'] as int,
        nextPageUrl: json['next_page_url'] as String?,
        prevPageUrl: json['prev_page_url'] as String?,
        complaints: (json['data'] as List<dynamic>?)
                ?.map((e) => Complaint.fromJson(e as Map<String, dynamic>))
                .toList() ??
            [],
      );
    }
  }